{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "functions": {
    "api/health.py": {
      "runtime": "python3.9"
    },
    "api/chat.py": {
      "runtime": "python3.9"
    },
    "api/logs.py": {
      "runtime": "python3.9"
    },
    "api/assessment.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "VITE_API_URL": "/api"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.py"
    }
  ]
}